{
    "manifest_version": 3,
    "name": "Abceed Shortcut Key",
    "version": "1.0.0",
    "description": "キーボードでabceedの学習ページにあるボタンを選択可能にする拡張機能",
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "*://*.abceed.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
